    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// Menghitung months_of_service
var months_of_service = 0;

if (hire_date != null &amp;&amp; evaluation_date != null) {
  var cal_hire = java.util.Calendar.getInstance();
  var cal_eval = java.util.Calendar.getInstance();

  cal_hire.setTime(hire_date);
  cal_eval.setTime(evaluation_date);

  var year_diff = cal_eval.get(java.util.Calendar.YEAR) - cal_hire.get(java.util.Calendar.YEAR);
  var month_diff = cal_eval.get(java.util.Calendar.MONTH) - cal_hire.get(java.util.Calendar.MONTH);

  months_of_service = (year_diff * 12) + month_diff;

  if (cal_eval.get(java.util.Calendar.DAY_OF_MONTH) &lt; cal_hire.get(java.util.Calendar.DAY_OF_MONTH)) {
    months_of_service = months_of_service - 1;
  }
}

// Menentukan apakah karyawan overdue atau tidak
var is_overdue = months_of_service >= promotion_duration ? 1 : 0;
</jsScript_script>
