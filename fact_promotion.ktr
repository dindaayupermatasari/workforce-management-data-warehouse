      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// Daftar grade disesuaikan dari tabel 'grade'
var grades = [
  { code: "GRD001", duration: 12, salary: 2500000 },
  { code: "GRD002", duration: 24, salary: 5000000 },
  { code: "GRD003", duration: 36, salary: 8000000 },
  { code: "GRD004", duration: 48, salary: 12000000 },
  { code: "GRD005", duration: 60, salary: 15000000 }
];

// Cari index grade saat ini
var currentIndex = -1;
for (var i = 0; i &lt; grades.length; i++) {
  if (grades[i].code == grade_code) {
    currentIndex = i;
    break;
  }
}

// Default: tidak ada perubahan
var next_grade_code = grade_code;
var next_salary = grades[currentIndex].salary;
var next_promotion_duration = grades[currentIndex].duration;

if (is_overdue == 1 &amp;&amp; currentIndex >= 0 &amp;&amp; currentIndex &lt; grades.length - 1) {
  // Jika overdue, naik ke grade berikutnya
  next_grade_code = grades[currentIndex + 1].code;
  next_salary = grades[currentIndex + 1].salary;
  next_promotion_duration = grades[currentIndex + 1].duration;
}
</jsScript_script>
